   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "# declarando as portas seriais\n",
    "ADC0.port = \"COM2\"\n",
    "\n",
    "ADC0.baudrate = 9600\n",
    "\n",
    "\n",
    "ADC0.timeout = 1\n",
    "\n",
    "\n",
    "ADC0.open()\n",
    "\n",
    "ADC0.write(b'C')\n",
    "\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
